pipeline {
  //  options {
 //    lock(label: 'node') 
   
   // }
    agent { label 'builder' }
     
 //  lock(label: 'node'){
    stages {
    
        stage('Build') {
     
            steps {
            sleep 6
                sh 'echo "Hello world!"'
            sleep 6
           }
           }
           
       stage ('Upload') {
            steps {
                rtUpload (
                    buildName: 'MK',
                    buildNumber: '48',
                    serverId: "SERVER_ID",
                    spec:
        """{
          "files": [
            {
              "pattern": "**/package.json",
              "target": "icrt-generic/'env.JOB_BASE_NAME'/"
            }
         ]
        }"""
                )
            }
        }
        
      }
}
